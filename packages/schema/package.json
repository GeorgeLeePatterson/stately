{
  "name": "@stately/schema",
  "version": "0.3.0",
  "description": "Type definitions and schema parser for Stately",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./generated": {
      "types": "./dist/generated.d.ts",
      "import": "./dist/generated.js"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "import": "./dist/helpers.js"
    },
    "./nodes": {
      "types": "./dist/nodes.d.ts",
      "import": "./dist/nodes.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js"
    },
    "./stately": {
      "types": "./dist/stately.d.ts",
      "import": "./dist/stately.js"
    },
    "./core/augment": {
      "types": "./dist/core/augment.d.ts",
      "import": "./dist/core/augment.js"
    },
    "./core/nodes": {
      "types": "./dist/core/nodes.d.ts",
      "import": "./dist/core/nodes.js"
    },
    "./core/plugin": {
      "types": "./dist/core/plugin.d.ts",
      "import": "./dist/core/plugin.js"
    },
    "./core/utils": {
      "types": "./dist/core/utils.d.ts",
      "import": "./dist/core/utils.js"
    },
    "./core/validation": {
      "types": "./dist/core/validation.d.ts",
      "import": "./dist/core/validation.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist .turbo",
    "typecheck": "tsc --noEmit",
    "test": "tsc --noEmit tests/*.test.ts",
    "dev": "tsc --build --watch"
  },
  "keywords": [
    "stately",
    "schema",
    "types",
    "openapi"
  ],
  "author": "George Lee Patterson <patterson.george@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/georgeleepatterson/stately.git",
    "directory": "packages/schema"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "openapi-types": "^12.1.3"
  }
}

{
  "name": "@statelyjs/schema",
  "version": "0.4.1",
  "description": "Type definitions and schema parser for Stately",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "check": "tsc --noEmit",
    "clean": "rm -rf dist .turbo",
    "dev": "tsc --build --watch",
    "tarball": "rm statelyjs-schema-*.tgz || true && pnpm --filter @statelyjs/schema pack"
  },
  "keywords": [
    "stately",
    "schema",
    "types",
    "openapi"
  ],
  "author": "George Lee Patterson <patterson.george@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/georgeleepatterson/stately.git",
    "directory": "packages/schema"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "openapi-types": "^12.1.3",
    "openapi-typescript-helpers": "^0.0.15"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js"
    },
    "./generated": {
      "types": "./dist/generated.d.ts",
      "import": "./dist/generated.js"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "import": "./dist/helpers.js"
    },
    "./nodes": {
      "types": "./dist/nodes.d.ts",
      "import": "./dist/nodes.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js"
    },
    "./stately": {
      "types": "./dist/stately.d.ts",
      "import": "./dist/stately.js"
    }
  }
}

{
  "name": "@stately/ui",
  "version": "0.3.0",
  "description": "React UI components for Stately state management",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./routes": {
      "types": "./dist/routes/index.d.ts",
      "import": "./dist/routes/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --build",
    "clean": "rm -rf dist .turbo",
    "typecheck": "tsc --noEmit",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "stately",
    "react",
    "ui",
    "components",
    "state-management"
  ],
  "author": "George Lee Patterson <patterson.george@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/georgeleepatterson/stately.git",
    "directory": "packages/ui"
  },
  "dependencies": {
    "@stately/schema-types": "workspace:*"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-router": "^1.0.0",
    "openapi-fetch": "^0.14.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.62.12",
    "@tanstack/react-router": "^1.132.35",
    "@types/node": "^22.10.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "openapi-fetch": "^0.14.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  }
}

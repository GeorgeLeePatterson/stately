{
  "name": "@stately/schema-types",
  "version": "0.3.0",
  "description": "Type definitions for Stately schema nodes",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
